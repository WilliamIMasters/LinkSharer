<html>
   <head>
      <title><%= title %></title>
      <link href="../css/style.css" rel="stylesheet" type="text/css">
   </head>
   <body>
      <h1><%= title %></h1>
      <p><%= desc %></p>

      <% if(link.substring(link.length -4).toLowerCase() == ".png" || link.substring(link.length -4).toLowerCase() == ".jpg" || link.substring(link.length -5).toLowerCase() == ".jpeg"){ %>
         <h1>image</h1>
         <img src="<%= link %>"></img>
      <% } else{ %>
         <h1>link</h1>
         <a href="<%= link %>"><%= link %></a>
      <% } %>
      <!-- <img src="<%= link %>"></img> -->
   </body>
</html>

